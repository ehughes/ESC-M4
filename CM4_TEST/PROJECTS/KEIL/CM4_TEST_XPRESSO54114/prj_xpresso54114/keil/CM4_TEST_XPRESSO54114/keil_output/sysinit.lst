Nstatic INLINE void Chip_I2C_EnableInt(LPC_I2C_T *pI2C, uint32_t intEn)
Xstatic __inline void Chip_I2C_EnableInt(LPC_I2C_T *pI2C, uint32_t intEn)
N{
N	pI2C->INTENSET = intEn;
